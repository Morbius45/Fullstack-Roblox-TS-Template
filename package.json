{
  "name": "roblox-ts-template",
  "version": "1.0.0",
  "description": "Roblox-TS template with Flamework, Lapis, Remo, Reflex, React, Mantle, and Asphalt",
  "main": "out/init.lua",
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "serve": "rojo serve",
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "mantle:deploy": "mantle deploy",
    "mantle:outputs": "mantle outputs",
    "asphalt:sync": "asphalt sync --target roblox",
    "asphalt:sync-dev": "asphalt sync --target development"
  },
  "keywords": [
    "roblox",
    "roblox-ts",
    "flamework",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rbxts/compiler-types": "3.0.0-types.0",
    "@rbxts/types": "^1.0.827",
    "concurrently": "^8.2.2",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@rbxts/lapis": "^0.3.6",
    "@rbxts/matter": "^0.8.5",
    "@rbxts/object-utils": "^1.0.4",
    "@rbxts/plasma": "^0.4.5",
    "@rbxts/pretty-react-hooks": "^0.6.4",
    "@rbxts/react": "^17.3.7-ts.1",
    "@rbxts/react-reflex": "^0.3.7",
    "@rbxts/react-ripple": "^3.0.1",
    "@rbxts/react-roblox": "^17.3.7-ts.1",
    "@rbxts/reflex": "^4.3.1",
    "@rbxts/remo": "^1.5.3",
    "@rbxts/ripple": "^0.10.1",
    "@rbxts/services": "^1.5.4",
    "@rbxts/signal": "^1.1.1",
    "@rbxts/t": "^3.2.1",
    "@rbxts/trove": "^1.3.0",
    "@rbxts/validate-tree": "^2.0.2",
    "eslint": "^9.39.2"
  }
}
